{
  "name": "npm-registry-browser",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "axios-cache-adapter": "^2.0.0",
    "highlight.js": "^9.12.0",
    "hoist-non-react-statics": "^2.5.0",
    "invariant": "^2.2.3",
    "js-md5": "^0.7.3",
    "material-ui": "^1.0.0-beta.37",
    "material-ui-icons": "^1.0.0-beta.36",
    "prop-types": "^15.6.1",
    "react": "^16.3.0-alpha.2",
    "react-dom": "^16.3.0-alpha.2",
    "react-markdown": "^3.3.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "recompose": "^0.26.0",
    "relative-date": "^1.1.3",
    "semver-match": "^0.1.1",
    "warning": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run test:watch",
    "test:watch": "react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom",
    "test:precommit": "npm run test:ci",
    "eject": "react-scripts eject",
    "lint": "npx eslint .",
    "pretty": "npx prettier --write '**/*.{js,jsx,json,css,scss}'",
    "precommit": "lint-staged && npm run lint && npm run test:precommit",
    "proxy-apis": "node ./bin/cors-anywhere.js"
  },
  "lint-staged": {
    "**/*.{js,jsx,json,css,scss}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "cors-anywhere": "^0.4.1",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "1.11.1"
  },
  "proxy": {
    "/api/npm-registry": {
      "target": "https://registry.npmjs.org",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/npm-registry": ""
      }
    },
    "/api/npm-api": {
      "target": "https://api.npmjs.org",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/npm-api": ""
      }
    }
  },
  "homepage": "."
}
